{
  "name": "google-ads-cli",
  "version": "1.0.0",
  "description": "CLI for Google Ads API with Node.js. Access campaigns, keywords, and Keyword Planner tools.",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "google-ads-cli": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cli",
    "nodejs",
    "typescript",
    "google-ads",
    "oauth2",
    "keyword-planner",
    "adwords",
    "advertising"
  ],
  "author": "Hayden Cassar",
  "license": "MIT",
  "dependencies": {
    "google-ads-api": "^17.0.0",
    "google-auth-library": "^9.0.0",
    "commander": "^12.0.0",
    "inquirer": "^9.2.15",
    "conf": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "express": "^4.18.3",
    "open": "^10.0.4",
    "cli-table3": "^0.6.3",
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
